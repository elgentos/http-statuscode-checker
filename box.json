{
    "alias": "http-statuscode-checker.phar",
    "chmod": "0755",
    "compression": "GZ",
    "directories": [
        "src"
    ],
    "files": [
        "README.md",
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "Tests"
            ],
            "in": "vendor"
        }
    ],
    "main": "bin/http-statuscode-checker",
    "output": "dist/http-statuscode-checker.phar"
}
